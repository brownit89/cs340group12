<html>
{{#each foo}}
{{this}}
{{/each}}

<h3>Reading Current Cards:</h3>
<table>
	<thead>
		<th>Name:</th>
		<th>Rarity</th>
		<th>Description</th>
		<th>Mana Cost</th>
		<th>Card Type</th>
	</thead>
	<tbody>
		{{#each card}}
		<tr>
			<td>{{card_name}}</td>
			<td>{{rarity}}</td>
			<td>{{description}}</td>
			<td>{{mana_cost}}</td>
			<td>{{card_type}}</td>
		</tr>
		{{/each}}
	</tbody>
</table>
</html>
